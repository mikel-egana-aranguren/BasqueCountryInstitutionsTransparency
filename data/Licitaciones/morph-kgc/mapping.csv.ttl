@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <https://data.ehu.eus/>.
@prefix epo: <http://data.europa.eu/a4g/ontology#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rev: <http://purl.org/stuff/rev#>.
@prefix gtfs: <http://vocab.gtfs.org/terms#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix schema: <http://schema.org/>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix tro: <http://ehu.eus/tro#>.

:map_licitaciones_contratos rml:logicalSource :source_licitaciones .

:map_licitaciones_contratos rr:subjectMap :s_contratos.
:map_licitaciones_contratos rr:predicateObjectMap :pom_contratos.
:map_licitaciones_contratos rr:predicateObjectMap :pom_url.
:map_licitaciones_contratos rr:predicateObjectMap :pom_concepto.
:map_licitaciones_contratos rr:predicateObjectMap :pom_expediente.
:map_licitaciones_contratos rr:predicateObjectMap :pom_adjudicatario.

:source_licitaciones a rml:LogicalSource;
    rml:source "licitaciones_euskadi_5.csv";
    rml:referenceFormulation ql:CSV.

:s_contratos rr:template "https://data.ehu.eus/contract/{codigo_cpv}".    

:pom_contratos a rr:PredicateObjectMap.
:pom_contratos rr:predicateMap :pm_contratos.
:pom_contratos rr:objectMap :om_contratos.
:pm_contratos a rr:PredicateMap.
:pm_contratos rr:constant rdf:type.
:om_contratos a rr:ObjectMap;
    rr:constant epo:Contract;
    rr:termType rr:IRI.

:pom_url a rr:PredicateObjectMap.
:pom_url rr:predicateMap :pm_url.
:pom_url rr:objectMap :om_url.
:pm_url a rr:PredicateMap.
:pm_url rr:constant tro:url.
:om_url a rr:ObjectMap;
    rml:reference "url";
    rr:termType rr:Literal.

:pom_concepto a rr:PredicateObjectMap.
:pom_concepto rr:predicateMap :pm_concepto.
:pom_concepto rr:objectMap :om_concepto.
:pm_concepto a rr:PredicateMap.
:pm_concepto rr:constant rdfs:comment.
:om_concepto a rr:ObjectMap;
    rml:reference "concepto";
    rr:termType rr:Literal;
    rr:language "es".

:pom_expediente a rr:PredicateObjectMap.
:pom_expediente rr:predicateMap :pm_expediente.
:pom_expediente rr:objectMap :om_expediente.
:pm_expediente a rr:PredicateMap.
:pm_expediente rr:constant schema:identifier.
:om_expediente a rr:ObjectMap;
    rml:reference "expediente";
    rr:termType rr:Literal.

:pom_adjudicatario a rr:PredicateObjectMap.
:pom_adjudicatario rr:predicateMap :pm_adjudicatario.
:pom_adjudicatario rr:objectMap :om_adjudicatario.
:pm_adjudicatario a rr:PredicateMap.
:pm_adjudicatario rr:constant epo:bindsContractor.
:om_adjudicatario a rr:ObjectMap;
    rr:template "https://data.ehu.eus/bcitr/company/{adjudicatario}";
    rr:termType rr:IRI.



